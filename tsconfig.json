{
  "compilerOptions": {
    "module": "CommonJS",
    "declaration": false,
    "removeComments": true,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2015",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false,
    "emitDeclarationOnly": false,
    "esModuleInterop": true,
    "paths": {
      "@notify/rtechnotifier": [
        "libs/rtechnotifier/src"
      ],
      "@notify/rtechnotifier/*": [
        "libs/rtechnotifier/src/*"
      ],
      "@bridge/data-bridge": [
        "libs/data-bridge/src"
      ],
      "@bridge/data-bridge/*": [
        "libs/data-bridge/src/*"
      ],
      "@services/core-services": [
        "libs/core-services/src"
      ],
      "@services/core-services/*": [
        "libs/core-services/src/*"
      ],
      "@toolkit/core-toolkit": [
        "libs/core-toolkit/src"
      ],
      "@toolkit/core-toolkit/*": [
        "libs/core-toolkit/src/*"
      ],
      "@controller/core-controller": [
        "libs/core-controller/src"
      ],
      "@controller/core-controller/*": [
        "libs/core-controller/src/*"
      ],
      "@entity/entities": [
        "libs/entities/src"
      ],
      "@entity/entities/*": [
        "libs/entities/src/*"
      ]
    }
  }
}