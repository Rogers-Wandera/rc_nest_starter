{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@auth/auth-guards/(.*)": "<rootDir>/../libs/auth-guards/src/$1",
    "@auth/auth-guards": "<rootDir>/../libs/auth-guards/src",
    "@bridge/data-bridge/(.*)": "<rootDir>/../libs/data-bridge/src/$1",
    "@bridge/data-bridge": "<rootDir>/../libs/data-bridge/src",
    "@services/core-services/(.*)": "<rootDir>/../libs/core-services/src/$1",
    "@services/core-services": "<rootDir>/../libs/core-services/src",
    "@toolkit/core-toolkit/(.*)": "<rootDir>/../libs/core-toolkit/src/$1",
    "@toolkit/core-toolkit": "<rootDir>/../libs/core-toolkit/src",
    "@controller/core-controller/(.*)": "<rootDir>/../libs/core-controller/src/$1",
    "@controller/core-controller": "<rootDir>/../libs/core-controller/src",
    "@entity/entities/(.*)": "<rootDir>/../libs/entities/src/$1",
    "@entity/entities": "<rootDir>/../libs/entities/src",
    "@listener/bridge-listeners/(.*)": "<rootDir>/../libs/bridge-listeners/src/$1",
    "@listener/bridge-listeners": "<rootDir>/../libs/bridge-listeners/src"
  }
}